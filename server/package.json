{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --exit-child --ignore-watch node_modules src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "test:e2e": "jest --config jest-e2e.config.ts --detectOpenHandles",
    "test:unit": "jest --config jest-unit.config.ts --detectOpenHandles",
    "prestart": "rimraf dist && tsc"
  },
  "dependencies": {
    "@prisma/client": "4.3.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@swc/core": "^1.3.8",
    "@swc/jest": "^0.2.23",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.1.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^29.2.0",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.8.3"
  }
}
